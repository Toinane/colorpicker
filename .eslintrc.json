{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": ["eslint:recommended", "preact", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": ["scripts/", "dist/"],
    "rules": {}
}
